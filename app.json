{
  "name": "csvlint",
  "description": "",
  "scripts": {

  },
  "env": {
    "AWS_ACCESS_KEY": {
      "required": true
    },
    "AWS_BUCKET_NAME": {
      "required": true
    },
    "AWS_SECRET_ACCESS_KEY": {
      "required": true
    },
    "CSVLINT_SESSION_SECRET": {
      "generator": "secret"
    },
    "LANG": {
      "required": true
    },
